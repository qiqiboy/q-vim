{
  "coc.preferences.formatOnType": true,
  "codeLens.enable": true,
  "colors.filetypes": ["*"],
  "diagnostic.refreshOnInsertMode": true,
  "diagnostic.warningSign": "▸▸",
  "diagnostic.errorSign": ">>",
  "diagnostic.infoSign": "!!",
  "diagnostic.hintSign": "!*",
  "eslint.autoFixOnSave": false,
  "eslint.alwaysShowStatus": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "html",
    "vue"
  ],
  "htmldjango.formatting.provider": "none",
  "htmldjango.djlint.enableLint": true,
  "htmldjango.djlint.profile": "nunjucks",
  "notification.disabledProgressSources": ["*"],
  "outline.sortBy": "position",
  "outline.expandLevel": 10,
  "prettier.printWidth": 120,
  "prettier.tabWidth": 4,
  "prettier.trailingComma": "none",
  "prettier.bracketSameLine": true,
  "prettier.semi": true,
  "prettier.singleQuote": true,
  "prettier.arrowParens": "avoid",
  "semanticTokens.filetypes": ["*"],
  "suggest.maxCompleteItemCount": 100,
  "suggest.completionItemKindLabels": {
    "keyword": "\uf1de",
    "variable": "\ue79b",
    "value": "\uf89f",
    "operator": "\u03a8",
    "constructor": "\uf0ad",
    "function": "\u0192",
    "reference": "\ufa46",
    "constant": "\uf8fe",
    "method": "\uf09a",
    "struct": "\ufb44",
    "class": "\uf0e8",
    "interface": "\uf417",
    "text": "\ue612",
    "enum": "\uf435",
    "enumMember": "\uf02b",
    "module": "\uf40d",
    "color": "\ue22b",
    "property": "\ue624",
    "field": "\uf9be",
    "unit": "\uf475",
    "event": "\ufacd",
    "file": "\uf723",
    "folder": "\uf114",
    "snippet": "\ue60b",
    "typeParameter": "\uf728",
    "default": "\uf29c"
  },
  "snippets.extends": {
    "javascript": ["javascript", "html"],
    "javascriptreact": ["javascriptreact", "html"],
    "typescriptreact": ["typescriptreact", "html"]
  },
  "snippets.userSnippetsDirectory": "~/.vim/UltiSnips",
  "tsserver.formatOnType": true
}
